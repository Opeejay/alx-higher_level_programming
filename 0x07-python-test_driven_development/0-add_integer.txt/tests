print("test case 1: ", add_integer(2,2))
print("test case 2 : ", add_integer(4,3))
try:  
  print("test case 3 : ", add_integer("4",3))
except Exception as e:
  print(e)
  print("test case 4 : ", add_integer(4,3.5))
  print("test case 5 : ", add_integer(-4,3.5))
  print("test case 6 : ", add_integer(2)) # 100
